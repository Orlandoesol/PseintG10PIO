Algoritmo contarCaracteres
	Definir texxto Como Caracter
	cont <- 0
	Escribir "Ingrese un palabra: "
	Leer texxto
	//cinco 5             5    > 5
	Mientras Longitud(texxto) > cont Hacer
		cont <- cont + 1
		//4 = 4+ 1 -> 5
	FinMientras
	Escribir "Cantidad de letras: ",cont
	
	
FinAlgoritmo
